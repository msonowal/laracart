{
  "name": "msonowal/laracart",
  "description": "Laravel Shopping Cart",
  "keywords": ["laravel", "cart", "shopping", "ecommerce", "laracart", "database"],
  "license": "MIT",
  "authors": [
    {
      "name": "Manash Jyoti Sonowal",
      "email": "manash149@gmail.com"
    }
  ],
  "require": {
    "illuminate/support": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*",
    "illuminate/session": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*",
    "illuminate/cookie": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*"
  },
  "require-dev": {
  },
  "autoload": {
    "psr-4": {
      "Msonowal\\Laracart\\": "src/"
    }
  },
  "extra": {
        "branch-alias": {
            "dev-master": "0.0-dev"
        },
        "laravel": {
            "providers": [
                "Msonowal\\Laracart\\CartServiceProvider"
            ],
            "aliases": {
                "Cart": "Msonowal\\Laracart\\Facades\\Cart"
            }
        }
    },
  "minimum-stability": "stable"
}
